class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        if(s==""):
            return 0
        elif(len(s)==1):
            return 1
        mylist=[]
        lg=[]
        for i in range(len(s)):
            for j in range(i,len(s)):
                if s[j] not in mylist:
                    mylist.append(s[j])
                else:
                    lg.append(len(mylist))
                    mylist=[]
                    break
        return max(lg)            
                    
                
